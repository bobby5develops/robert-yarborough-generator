/*********************** Landing Page Rules for Product Pools *************************/
.finderPage#topgifts .product.notralphlauren .image{
    border: none !important;
}
.finderPage#topgifts .products{
    position: absolute;
    top: 127px;
    left: 80px;
    width: 800px;

}

.finderPage#topgifts .products .product{
    position: relative;
    display: inline-block;
    vertical-align:top;
    /*width:150px;*/
    /*height:210px;*/
    /*overflow: hidden;*/
    margin-left: 3px;
    margin-top: 19px;
    margin-bottom: 18px;
    left:61px;
    width: 130px;
    height: 136px;
}

.finderPage#topgifts .products .product:first-child{
    position: relative;
    width: 291px;
    height: 500px;
    display: block;
    /*overflow: hidden;*/
    float: left;

    left: 61px !important;
    margin-top: 16px !important;
    margin-left: -1px !important;

}


.finderPage#topgifts .products .product p.votes{
    position: absolute;
}


.finderPage#topgifts .products .product:first-child .image{
    width: 291px;
    height: 356px;
    overflow: hidden;
}

.finderPage#topgifts .products .product .image{
    width: 113px;
    height: 134px;
    overflow: hidden;
    /*float: right;*/

}


.finderPage#topgifts .products .num_container{
    position: absolute;
    top:0px;
    left:0px;
}


.finderPage#topgifts .products .num_container .indexcount{
    position: absolute;
    border-radius: 20px;
    background-color: #000000;
    color: #ffffff;
    border: 4px solid #ffffff;
    left: -15px;
    width: 23px;
    height: 23px;
    text-align: center;
    font-size: 14px;
    box-shadow: grey 1px 1px 1px;
    top: -14px;
}

.bigtab{
    width: 300px;
    height: 400px;
    position: relative;
    top: 0px;
    left: 100px;
}

.finderPage#topgifts #animewrapper .childrenabsolute img {
   top: 20px;
   left: 52px;
}




.finderPage#topgifts .products .product .votes {
   color: #ffffff;
   text-transform: uppercase;
   font-weight: bold;
   display: inline-block;

}

.finderPage#topgifts .products .product .votes img{
  display: inline-block !important;

}

.finderPage#topgifts .products .product:first-child .votes {
    display: block;

}




.finderPage#topgifts .products .product:first-child .product_image{
    padding-bottom: 0 !important;
    border: none !important;
    height: 370px;
    overflow: hidden;

}


.finderPage#topgifts .product .product_image {
    padding-bottom: 0 !important;
    border: none !important;
    height: 127px;
    overflow: hidden;

}

.finderPage#topgifts .products .product:nth-child(1n+2){
   position: relative;
   left: 84px;
}