/* CSS Document */

BODY {
	margin: 0px !important;
	padding: 0px !important;
	font-family: arial, verdana, sans-serif;
}

.invisible {
	visibility: hidden;
}

#finderContainer {
	width: 1024px;

	position: relative;

}

.finderPage {
	width: 100%;
	position: relative;
	top: 0px;
	left: 0px;
	overflow: hidden;
	z-index:1;
	visibility: hidden;
}

#pageContainer {
	width: 100%;
	min-height: 537px;
	position: relative;
	overflow: hidden;

}

.image_replacement, .ir {
	text-indent: -9000px;
	outline: 0px;
	border: 0px;
	overflow: hidden;
}

A.image_replacement:hover {
	background-position: bottom !important;
}

#finderContainer #bodyContainer {
	position: relative;
	overflow: hidden;
    z-index: 0;
}

#finderContainer #bodyContainer .loading {
	position: absolute;
	background: #efefef;
	top: 0px;
	left: 0px;
	z-index: 595;
	width: 100%;
}

#finderContainer #bodyContainer #overlay {
	position: absolute;
	left: 0px;
	z-index: 595;
	width: 100%;
	height: 1200px;
	background: white;
	-moz-opacity: 0;
	-webkit-opacity: 0;
	-o-opacity: 0;
	opacity: 0;
	filter: alpha(opacity=70);
	zoom:1;
}



.finderPage #pageOverlay {
	position: absolute;
	left: 0px;
	z-index: 99;
	width: 100%;
	height: 100%;
	background: white;
	-moz-opacity: .7;
	-webkit-opacity: .7;
	-o-opacity: .7;
	opacity: .7;
	filter: alpha(opacity=70);
	zoom:1;
}


#finderContainer #bodyContainer .loading #loadingMessage {
	position: absolute;
	top: 100px;
	left: 306px;
	background: url("../images/loading.png");
	color: white;
	z-index: 999;
	width: 288px;
	height: 168px;
	text-align: center;
}

#finderContainer #bodyContainer .loading #loadingMessage IMG {
	margin-top: 100px;
}

#finderContainer #footer {


}




.being_animated * {
	visibility: inherit;
}

.poolContainer {
	position: relative;
	width: 1007px;
	margin: 0 auto;
}

.videobox {
	position: relative;
	display: inline-block;
	width: 500px;
	height: 500px;
	border: 10px solid #000;
}


A {
	outline: 0px;
}






#navContainer {
    position: relative;
    z-index: 1;
}

#navContainer img {
    display: block;
}

#navContainer .dropdownimage {
    display: none;
    position: absolute;
    top: 0;
}

#navContainer .dropdownimage.active {
    display: block;
}




/* buttonbucket is for the buttons in the black bar top nav */
#navContainer .buttonbucket {
    position: absolute;
    top: 0;
    height: 40px;
    width: 1024px;
}
#navContainer .buttonbucket .navitem {
    display: block;
    height: 100%;
    float: left;
    cursor: pointer;
    background-color: #cc0000;
    filter: alpha(opacity = 0);
    opacity: 0.0;
}





#footer {

}




#poolResults {
	clear: both;
	-webkit-transform: translate(0px, 0px, 0px);
	position: relative;
	overflow: hidden;
	width: 590px;
	height: 600px;
}

.products {
	width: 1005px;
	margin: 0 auto;
	position: relative;
}

#poolHeader {

	width: 300px;
	height: 20px;	
	font-family: Conv_ITCAvantGardeStd-Md, arial;
	font-size: 12px;
}

#poolHeader DIV {
	float: left;
}

#poolHeader DIV#filterDiv {
	margin-left: 20px;
	
}

.poolPagination {
	text-align: right;
	font: 14px Arial;
	color: #333333;
	margin-top: 10px;
	margin-bottom: 10px;
}

.poolPagination .itemsFound {

	float: left;
}

.poolPagination .pageNumbers {
	float: right;
	text-align:right;
}

.poolPagination .pageNumbers A {
	color: #333333;
	text-decoration: none;
	padding-left: 3px;
	padding-right: 3px;
}

.poolPagination .pageNumbers A:hover, .poolPagination .pageNumbers A.selected {
	color: #CC0000;
	text-decoration: underline !important;
}





.product.notralphlauren {
	
	display: inline-block;
	*display: inline;
	zoom: 1;
	vertical-align: top;
	margin: 12px;	
	width: 172px;
	font: 11px Verdana, Arial;
	position: relative;
	line-height: 1.3;

	box-sizing: border-box;
	_webkit-box-sizing: border-box;

	-webkit-transition: opacity 0.5s; /* For Safari 3.1 to 6.0 */
    transition: opacity 0.5s;
}

.product.notralphlauren .addToBridalParty {
	cursor: pointer;
	margin-top: 10px;
	margin-bottom: 5px;
}

.product.notralphlauren .colorSwatches {
	margin-top: 10px;
}

.product.notralphlauren.hasPromotions .bonusOffers {
	display: block;	
}

.product.notralphlauren .bonusOffers {
	padding-top: 10px;
	background: #ffffff;
	color: #cc0000;
	font-weight: bold;
	border-top: 1px solid #efefef;
	margin-top: 10px;
	position: relative;
	display: none;
	cursor: pointer
}

.product.notralphlauren .bonusOffers .offer {
	font-weight: normal;
	color: #cc0000;
	cursor: auto;
	padding: 5px;
}

.product.notralphlauren .price {
	margin: 3px 0;
}

.product.notralphlauren .votes {
	margin: 3px 0;
}

.product.notralphlauren .giftListRow {
	cursor: pointer;
}

.product.notralphlauren.inWishlist .giftListRow {
	display: none;
}

.product.notralphlauren .giftListRow2 {
	display: none;
	color: #999;
}

.product.notralphlauren.inWishlist .giftListRow2 {
	display: block;
}

.product.notralphlauren .giftListRow, .product.notralphlauren .giftListRow2 {
	font-weight: bold;
}

.product.notralphlauren .quickview {
	position: absolute;
	background-color: #fff;
	opacity: .7;
	height: 26px;
	line-height: 26px;
	left: 50%;
	bottom: 8px;
	margin-left: -44px;
	width: 88px;
	display: inline-block;
	text-align: center;
	overflow: hidden;
	cursor: pointer;
	-webkit-border-radius: 12px;
	-moz-border-radius: 12px; 
	border-radius: 12px;

	-webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
	-moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
	box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
}

.product.notralphlauren .quickview.hover {
	opacity: 1 !important;
}

.touch .product.notralphlauren .quickview {
	display: block !important;
}

.product.notralphlauren .quickview A {
	display: block;
	background: url('../images/qv-button.png');
	width: 88px;
	height: 26px;
	text-indent: -9000px;
}

 .product.notralphlauren.hover .quickview {
 	display: block;
 }

.product.notralphlauren .quickview A:hover {
	background-position: 0px -26px;
}

.product.notralphlauren .quickview A.clicked {
	background-position: 0px -52px !important;
}


.product.notralphlauren .product_image {
	padding-bottom: 122.63%;
	border: 1px solid #cecece;
	position: relative;
}


.product.notralphlauren .image {
	width: 170px;
	overflow: hidden;
	position: absolute;
	display: block;
}

.product.notralphlauren .moreColors {
	text-align: center;
	background: black;
	color: white;
	font: 10px Arial;
	text-transform: uppercase;
	padding-top: 2px;
	padding-bottom: 2px;
	font-weight: bold;
}

.product.notralphlauren A {
	text-decoration: none;
}

.product.notralphlauren .product.notralphlaurenImage {
	width: 171px;	
	height: 211px;
}



.product.notralphlauren .votingRow {
	padding-top: 5px;
	font-family: Conv_ITCAvantGardeStd-Md, Arial;
	position: relative;
	cursor: pointer;
}

.product.notralphlauren .votingRow{
	color: #000000;
	font-size:12px;
	font-weight: bold;
}

.product.notralphlauren.voted .votingRow .button {
	color: #999999;
}

.product.notralphlauren.voted .votingRow {
	cursor: default;
}

.product.notralphlauren .voteCount{
	color: #000000;
	font-size: 9px;
	white-space: nowrap;
	position: absolute;
	top: 7px;
	font-family: verdana, arial;
	font-weight: normal;
}

.product.notralphlauren .loveit{

}


.product.notralphlauren .label {
	clear:both;
	padding-top: 8px;
}

.product.notralphlauren .label A {
	color: black;
	text-decoration: none;
}

.product.notralphlauren .price1 {
	font-weight: bold;
}

.product.notralphlauren .price2 {
	font-weight: bold;
}

.product.notralphlauren .price3 {
	font-weight: bold;
	color: red;
}

.product.notralphlauren:HOVER {

}

.product.notralphlauren .voteButton {
	cursor: pointer;
}

.product.notralphlauren .buttons {
	margin: 6px 0 0 0;
}

