    left: 70px;
}

.finderPage#dressing101 .checkList .li {
    height: 30px;
}

.finderPage#dressing101 #suit img, #shirt img {
    position: absolute;
    top: 25px;
    left: 16px;
}

.finderPage#dressing101 #closeChecllist {
    position: absolute;
    top: 20px;
    left: 667px;
    cursor: pointer;
}

.finderPage#dressing101 #printChecllist {
    position: absolute;
    top: 500px;
    left: 330px;
    cursor: pointer;
    display: none;
}

.finderPage#dressing101 #forPrint {
    width: 500px;
    height: 527px;
    position: absolute;
    top: 20px;
    left: 400px;
    z-index: 0;
}

.finderPage#dressing101 #printMe {
    position: absolute;
    top: 2330px;
    left: 4030px;
}