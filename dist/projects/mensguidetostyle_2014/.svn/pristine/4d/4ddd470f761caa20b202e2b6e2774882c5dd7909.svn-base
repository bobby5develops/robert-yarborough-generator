#finderContainer #navContainer {
    background: transparent;
    height: 36px;
    color: #00427a;
    display: block;
	position: relative;
	z-index: 200;
}
#finderContainer #navContainer #navBack{
    background-position: 0px 0px;
    width: 898px;
    height: 36px;
}
#finderContainer #navContainer ul#navBar {
    width: 898px;
    height: 36px;
    margin: 0px;
    padding: 0px;
    background: #000;
}

#finderContainer #navContainer ul#navBar li{
    text-indent: -9999px;
    height: 35px;
    background: url('../images/nav/nav.png') no-repeat;
    float: left;
    display:block;
}

#finderContainer #navContainer ul#navBar li a{
    width:100%;
    height:100%;
    list-style: none;
    display: block;
}

#finderContainer #navContainer ul#navBar li.highlighted > a {
    cursor:default;
}


#finderContainer #navContainer ul#navBar li#home a{
    width:336px;
    height:36px;
    display: block;
}

/*=================== HOW TO WEAR IT ==============================*/

#finderContainer #navContainer ul#navBar li#howtowearit{
    background-position: -336px 0;
    width:144px;
    height:36px;
    display: block;
}

#finderContainer #navContainer ul#navBar li#howtowearit:hover,
#finderContainer #navContainer ul#navBar li#howtowearit.highlighted{
    background-position: -336px -36px;
}

/*====================== HOW TO WEAR IT SUBNAV ===============================*/

#finderContainer #navContainer ul#navBar li#howtowearit:hover > ul{
    display:block;
}

#finderContainer #navContainer ul#navBar li#howtowearit ul{
    position: absolute;
    top: 37px;
    left: 245px;
    margin: 0px;
    padding: 0px 10px;
    width: 567px;
    height: 31px;
    background: url(../images/nav/subnavbak.png) left bottom no-repeat;
    list-style-type: none;
    display:none;
    padding-top: 0;
}
#finderContainer #navContainer ul#navBar li#howtowearit li.arrowdown {
    width: 19px !important;
    height: 12px !important;
    background: url(../images/nav/sub_arrowDown.png) 0 0px no-repeat !important;
    display: block !important;
    margin: 0 !important;
    border: 0 !important;
    padding: 0 !important;
    position:absolute !important;
    top: -3px !important;
    left: 170px !important;
}

#finderContainer #navContainer ul#navBar li#howtowearit ul li{
    height:20px;
    background: url(../images/nav/subnav.png) no-repeat;
    margin-left:1px;
    margin-top: 6px;
}

/*========== changes the cursor for the highlighted item in the subnav*/
#finderContainer #navContainer ul#navBar li#howtowearit ul .highlighted a{
    cursor: default;
}
