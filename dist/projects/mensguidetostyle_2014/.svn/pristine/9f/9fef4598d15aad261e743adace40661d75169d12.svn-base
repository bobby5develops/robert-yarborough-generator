.finderPage#howtowearit {
    width:898px;
    font-family: 'helvetica','arial', san-serif;
   }

.finderPage#howtowearit .title.first {
    position: absolute;
    left: 52px;
    top: 47px;
}
.finderPage#howtowearit .title.second {
    position: absolute;
    left: 52px;
    top: 285px;
}

.finderPage#howtowearit .linkClick {
    position: absolute;
    top: 330px;
}

/*linkClick hover-state*/
.finderPage#howtowearit .linkClick a{
    position: absolute;
    width: 105px;
    height: 211px;
}

.finderPage#howtowearit .linkClick a:hover .hoverborder{
    width: 101px;
    height: 178px;
    position:absolute;
    top:0px;
    left:0px;
    border:3px solid #000000;
    z-index: 1;
}





