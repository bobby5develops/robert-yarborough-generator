#finderContainer #navContainer {
    background: transparent;
    height: 36px;
    color: #00427a;
    display: block;
	position: relative;
	z-index: 200;
}
#finderContainer #navContainer #navBack{
    background-position: 0px 0px;
    width: 898px;
    height: 36px;
}
#finderContainer #navContainer ul#navBar {
    width: 898px;
    height: 36px;
    margin: 0px;
    padding: 0px;
    background: #000;
}

#finderContainer #navContainer ul#navBar li{
    text-indent: -9999px;
    height: 35px;
    background: url('../images/nav/nav.png') no-repeat;
    float: left;
    display:block;
}

#finderContainer #navContainer ul#navBar li a{
    width:100%;
    height:100%;
    list-style: none;
    display: block;
}

#finderContainer #navContainer ul#navBar li.highlighted > a {
    cursor:default;
}


